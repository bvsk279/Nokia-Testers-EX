{
    "author": "Siva Krishna Bellam",
    "manifest_version": 2,
    "name": "Nokia Testers Tool",
    "description": "Experiance the New Normal",
    "version": "1.0",
    "icons": {"128": "icons/favicon-192.png"},
    "browser_action":{
        "default_icon": "icons/favicon-32.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "activeTab",
        "storage",
        "\u003Call_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "content_scripts": [
        {
            "css": ["fontawsome.css","content-includes/content-styles.css"],
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_idle",
            "js": [
                "content-includes/jquery.js",
                "content-includes/functions.js",
                "content-includes/content-scripts/UTE_Cloud.js",
                "content-includes/content-scripts/RepPortal.js"
            ]
        }
    ],
    "background": {
        "persistent": true,
        "scripts": [
            "content-includes/jquery.js",
            "bg-includes/HandleCors.js",
            "bg-includes/background.js"
        ]
    }
    
}

